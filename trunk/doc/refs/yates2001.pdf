<script> 
function Init(){
	DocURL = document.URL;
	protocolIndex=DocURL.indexOf("://",4);
	serverIndex=DocURL.indexOf("/",protocolIndex + 3);
	BeginURL=DocURL.indexOf("#",1) + 1;
	urlresult=DocURL.substring(BeginURL,serverIndex);				
	displayresult=DocURL.substring(protocolIndex + 3 ,serverIndex);

}
function Homepage(){
<!--
	InsertElementAnchor(urlresult, displayresult);
}
Init();
RedirectOnTilde();

function RedirectOnTilde()
{
	urlresultsize = urlresult.length ;
	docurlsize    = DocURL.length;	
	if (docurlsize < urlresultsize+5){return;}
	if (DocURL.charAt(urlresultsize+1)  == '~'){
		newurl = urlresult+"/" +DocURL.charAt(urlresultsize+2) + "/"+ DocURL.charAt(urlresultsize+3) + "/" +DocURL.substr(urlresultsize+2,docurlsize) ;
		window.location.href=newurl;
	}
	if (docurlsize < urlresultsize+12){	return;	}
	if (DocURL.charAt(urlresultsize+2)  != '/' && DocURL.charAt(urlresultsize+4) == '/'){
		newurl = urlresult+ DocURL.substr(urlresultsize+4,docurlsize) ;
		window.location.href=newurl;
	}	
}
function HtmlEncode(text){
    return text.replace(/&/g, '&amp').replace(/'/g, '&quot;').replace(/</g, '&lt;').replace(/>/g, '&gt;');
}
function TagAttrib(name, value){
    return ' '+name+'="'+HtmlEncode(value)+'"';
}

function PrintTag(tagName, needCloseTag, attrib, inner){
    document.write( '<' + tagName + attrib + '>' + HtmlEncode(inner) );
    if (needCloseTag) document.write( '</' + tagName +'>' );
}
function URI(href){
    IEVer = window.navigator.appVersion;
    IEVer = IEVer.substr( IEVer.indexOf('MSIE') + 5, 3 );
    return (IEVer.charAt(1)=='.' && IEVer >= '5.5') ?
        encodeURI(href) :
        escape(href).replace(/%3A/g, ':').replace(/%3B/g, ';');
}
function InsertElementAnchor(href, text){
    PrintTag('A', true, TagAttrib('HREF', URI(href)), text);
}

//-->
</script>
